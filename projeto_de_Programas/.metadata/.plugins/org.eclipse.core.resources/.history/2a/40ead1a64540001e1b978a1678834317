public class EnsalamentoMain {
	public static void main(String[] args) {
        // Criando objetos Turma
        Turma turma1 = new Turma("Algoritmos e Estrutura de Dados I", "Edleno Silva", 60, true);

        // Adicionando horários às turmas
        turma1.addHorario(1); // Segunda 8hs
        turma1.addHorario(15); // Quarta 8hs
        turma1.addHorario(29); // Sexta 8hs


        // Exibindo descrição das turmas
        System.out.println(turma1.getDescricao());

    }
}